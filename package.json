{
  "name": "@nordnet/ui",
  "version": "0.0.1",
  "author": "Nordnet <frontend@nordnet.se> (https://nordnet.se)",
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "types",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint:js": "eslint --ext=.jsx --ext=.js src",
    "lint:css": "stylelint './src/**/*.{js,jsx}'",
    "lint": "yarn run lint:js && yarn run lint:css",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "babel:esm": "BABEL_ENV=esm babel -d dist/esm src/",
    "babel:cjs": "babel -d dist/cjs src/",
    "babel": "babel src -d dist --copy-files --ignore .md"
  },
  "dependencies": {
    "nordnet-ui-kit": "^4.0.5",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16",
    "react-dom": ">=16",
    "react-jss": "^8.6.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@nordnet/prettier-config": "2.0.0",
    "@storybook/addon-a11y": "5.0.1",
    "@storybook/addon-actions": "5.0.1",
    "@storybook/addon-links": "5.0.1",
    "@storybook/addons": "5.0.1",
    "@storybook/react": "5.0.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "5.15.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.5.1",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2"
  }
}
